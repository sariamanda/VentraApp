<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools ="http://schemas.android.com/tools"
    tools:context=".activity.MainActivity"
    android:background="@drawable/home"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/button5"
            android:layout_width="90dp"
            android:layout_height="78dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/btn_trxpth"
            app:layout_constraintEnd_toStartOf="@+id/button6"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints,OnClick"
            tools:layout_editor_absoluteY="-7dp" />

        <ImageView
            android:id="@+id/button6"
            android:onClick="clickdeposit"
            android:layout_width="66dp"
            android:layout_height="68dp"
            android:layout_marginLeft="7dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/btn_deposit"
            app:layout_constraintEnd_toStartOf="@+id/button7"
            tools:layout_editor_absoluteY="-1dp"
            tools:ignore="MissingConstraints,OnClick" />

        <ImageView
            android:id="@+id/button7"
            android:onClick="clickprofil"
            android:layout_width="62dp"
            android:layout_height="69dp"
            android:layout_marginLeft="5dp"
            android:layout_marginEnd="24dp"
            android:background="@drawable/btn_profil"
            app:layout_constraintEnd_toStartOf="@+id/button8"
            tools:layout_editor_absoluteY="6dp"
            tools:ignore="MissingConstraints,OnClick" />

        <ImageView
            android:id="@+id/button8"
            android:onClick="clickhistory"
            android:layout_width="60dp"
            android:layout_height="67dp"
            android:layout_marginLeft="5dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/btn_history"
            app:layout_constraintEnd_toStartOf="@+id/button9"
            tools:layout_editor_absoluteY="4dp"
            tools:ignore="MissingConstraints,OnClick" />

        <ImageView
            android:id="@+id/button9"
            android:onClick="clicknotice"
            android:layout_width="58dp"
            android:layout_height="67dp"
            android:layout_marginLeft="5dp"
            android:layout_marginEnd="4dp"
            android:background="@drawable/btn_notice"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="7dp"
            tools:ignore="MissingConstraints,OnClick" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>