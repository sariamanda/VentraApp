<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/notice"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/button20"
            android:onClick="clicktrx"
            android:layout_width="85dp"
            android:layout_height="71dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/btn_trx"
            tools:layout_editor_absoluteX="7dp"
            tools:layout_editor_absoluteY="1dp"
            tools:ignore="ContentDescription,MissingConstraints,OnClick" />

        <ImageView
            android:id="@+id/button21"
            android:onClick="clickdeposit"
            android:layout_width="75dp"
            android:layout_height="71dp"
            android:layout_marginLeft="8dp"
            android:background="@drawable/btn_deposit"
            tools:ignore="ContentDescription,MissingConstraints,OnClick"
            tools:layout_editor_absoluteX="96dp"
            tools:layout_editor_absoluteY="6dp" />

        <ImageView
            android:id="@+id/button22"
            android:onClick="clickprofil"
            android:layout_width="65dp"
            android:layout_height="71dp"
            android:layout_marginLeft="8dp"
            android:background="@drawable/btn_profil"
            tools:ignore="ContentDescription,MissingConstraints,OnClick"
            tools:layout_editor_absoluteX="185dp"
            tools:layout_editor_absoluteY="1dp" />

        <ImageView
            android:id="@+id/button23"
            android:onClick="clickhistory"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="8dp"
            android:background="@drawable/btn_history"
            tools:ignore="ContentDescription,MissingConstraints,OnClick"
            tools:layout_editor_absoluteX="259dp" />

        <ImageView
            android:id="@+id/button24"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="6dp"
            android:background="@drawable/btn_noticepth"
            tools:ignore="ContentDescription,MissingConstraints,OnClick"
            tools:layout_editor_absoluteX="325dp"
            tools:layout_editor_absoluteY="0dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>